{
	"name": "gigadrive/markdown-wiki-bundle",
	"version": "1.3.1",
	"type": "symfony-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Mehdi Baaboura",
			"email": "mbaaboura@gigadrivegroup.com"
		}
	],
	"require": {
		"php": "^8.0",
		"gigadrive/php-commons": "^1.1",
		"symfony/framework-bundle": "~5.0",
		"doctrine/annotations": "^1.13",
		"symfony/yaml": "^5.3",
		"ext-json": "*",
		"erusev/parsedown": "^1.7",
		"symfony/finder": "^5.3",
		"symfony/cache": "^5.3",
		"symfony/http-kernel": "^5.3",
		"symfony/console": "^5.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"symfony/phpunit-bridge": "^5.3",
		"nyholm/symfony-bundle-test": "^1.8",
		"matthiasnoback/symfony-dependency-injection-test": "^4.3",
		"symfony/browser-kit": "^5.3",
		"symfony/twig-bundle": "^5.3",
		"symfony/css-selector": "^5.3"
	},
	"readme": "README.md",
	"homepage": "https://github.com/Gigadrive/MarkdownWikiBundle",
	"autoload": {
		"psr-4": {
			"Gigadrive\\Bundle\\MarkdownWikiBundle\\": "src/"
		},
		"exclude-from-classmap": [
			"/tests/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Gigadrive\\Bundle\\MarkdownWikiBundle\\Tests\\": "tests"
		}
	}
}
